{
    "intents": [
        {
            "name": "PlayIntent",
            "samples": [
                "play",
                "lire la musique",
                "jouer {de|} {la musique|}",
                "lancer {la musique|}",
                "{mettre} la musique"
            ]
        },
        {
            "name": "PauseIntent",
            "samples": [
                "pause",
                "mettre {sur|} pause",
                "stop",
                "arrête",
                "arrêter"
            ]
        },
        {
            "name": "SkipNextIntent",
            "samples": [
                "suivant",
                "{musique|} suivante"
            ]
        },
        {
            "name": "SkipPreviousIntent",
            "samples": [
                "précédent",
                "{musique|} précédente"
            ]
        },
        {
            "name": "VolumeLevelIntent",
            "slots": {
                "VOLUMELEVEL": "AMAZON.NUMBER"
            },
            "samples": [
                "{mettre|changer|ajuster|} le volume {sur|à} {-|VOLUMELEVEL}",
                "volume {sur|à|} {-|VOLUMELEVEL}"
            ]
        },
        {
            "name": "GetDevicesIntent",
            "samples": [
                "liste",
                "liste des appareils",
                "{lister|chercher|trouver} les appareils",
                "lister",
                "appareils"
            ]
        },
        {
            "name": "DevicePlayIntent",
            "slots": {
                "DEVICENUMBER": "AMAZON.NUMBER"
            },
            "samples": [
                "{jouer|commencer|lancer|met} {de|} {la musique|spotify} sur {l'appareil|le numéro|} {-|DEVICENUMBER}"
            ]
        },
        {
            "name": "DeviceTransferIntent",
            "slots": {
                "DEVICENUMBER": "AMAZON.NUMBER"
            },
            "samples": [
                "passer {la musique|la lecture|} sur {l'appareil|le numéro|} {-|DEVICENUMBER}",
                "écouter {de|} la musique sur {l'appareil|le numéro|} {-|DEVICENUMBER}"
            ]
        },
        {
            "name": "GetTrackIntent",
            "samples": [
                "qu'est ce qui est joué",
                "qu'est ce que j'écoute",
                "{c'est quoi|quelle est} cette musique",
                "{c'est quoi|quel est} ce {morceau|son}"
            ]
        }
    ]
}